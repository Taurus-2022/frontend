{
  "envId": "{{envId}}",
  "framework": {
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "installCommand": "npm install --prefer-offline --no-audit --progress=false",
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "ignore": [".git", ".github", "node_modules", "cloudbaserc.js","cloudbaserc.json"],
          "envVariables": {
            "stage": "{{stage}}"
          }
        }
      }
    }
  }
}